<%= form_with model: @address, url: "/address/update/#{@address.id}" do |form| %>
	<div class="row">
		<%= render partial: "shared/form_errors", locals: { object: form.object } %>
		<div class="col-md-12 ">
			<%= form.text_field :address, value: @address.address, required: true, class: "contactus", placeholder: "Address" %>
		</div>
		<div class="col-md-12 ">
			<%= form.text_field :contact, value: @address.contact, required: true, class: "contactus", placeholder: "Contact" %>
		</div>
		<div class="col-md-12 ">
			<%= form.submit "Update", class: "send_btn" %>
		</div>
	</div>
<% end %>